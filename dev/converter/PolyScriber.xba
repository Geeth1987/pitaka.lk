Sub PaliComposer
DialogLibraries.LoadLibrary(&quot;PolyScriber&quot;)
Dlg = CreateUnoDialog(DialogLibraries.PolyScriber.PaliComposer)
Dim edtPali as Object
Select Case Dlg.Execute() 
Case 1
 edtPali = Dlg.getControl(&quot;edtPali&quot;)
 msgBox edtPali.Text
 
End Select

end sub

Sub dlgPolyScriber

