Sub ConvertToSinhalaUnicode()
	&apos; First Convert Text into Pali
	Call ConvertSinhalaPaliChar
	Dim arstrRoman(ARRAY_SIZE_SINHALA) as string 
	Dim arstrSinhala(ARRAY_SIZE_SINHALA) as string
	Dim index as integer
	Dim iArraySize as Integer

	&apos;sinhala will get an array and then call replace for each array item
    iArraySize =	GetSinhalaFonts1(arstrRoman,arstrSinhala)
	
	For index =0 to iArraySize&apos;
		call ReplaceCSVStrings(arstrRoman(index), arstrSinhala(index),CASE_NO)
	next Index
end sub

