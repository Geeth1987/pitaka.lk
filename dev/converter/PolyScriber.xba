Sub ReplaceCSVStrings(strSource as string, strTarget as string, bCase as Boolean)
    dim sFontName as string
    


    Dim strError As String
    
    Dim arstrSource(ARRAY_SIZE_SINHALA) As String
    Dim arstrTarget(ARRAY_SIZE_SINHALA) As String
    
    Dim nCounter As Long
	Dim sMsg as String
    
    &apos; just to check if they read the manual not for any other purpose (shsh!!)
    &apos; check for &apos;@&apos; at the end of each line.. Very much Needed...
    If Right(strSource, 1)  &lt;&gt; &quot;@&quot; Or Right(strTarget, 1) &lt;&gt; &quot;@&quot; Then
        strError = &quot;@ expected at the end, please read the manual.&quot; + strSource + strTarget
        GoTo HandleError
    End If
    
    &apos; Check to see if the string length is less than 256
    &apos; which will never be parsed more than a 256 size array
    If Len(strSource) &gt; ARRAY_SIZE Then
        strError = &quot;Line 1 size is larger than 256 characters&quot;
        GoTo HandleError
    End If
    
    If Len(strTarget) &gt; ARRAY_SIZE Then
        strError = &quot;Line 2 size is larger than 256 characters&quot;
        GoTo HandleError
    End If
    
        
   
    Dim nArray As Long
    Dim nIndex As Long
    
    Dim lLength As Long
    lLength = Len(strSource)
    Dim strChar As String
    
    strChar = Mid(strSource, 1, 1)
    &apos; parse the string and put into arrays in order
    &apos; later make it so that it will end at the end of line or end of string!!
    nArray = 0
    nIndex = 1
    While strChar &lt;&gt; &quot;@&quot;
        &apos; sample a,b,.n,aa,ii,fontname@
        &apos; go through each character
        &apos; if &apos;,&apos; is not found then we must add to the same array number
        &apos; if &apos;,&apos; is found we must increment the array number
        &apos; the string number is always incremented
        
        If strChar = &quot;,&quot; Then
            &apos;skip to the next letter on line
            strChar = Mid(strSource, nIndex, 1)
            &apos;put in the next array as a new character for formating
            nArray = nArray + 1
        Else
            arstrSource(nArray) = arstrSource(nArray) + strChar
        End If
        
        &apos; prime the pump
        nIndex = nIndex + 1
        strChar = Mid(strSource, nIndex, 1)
    Wend
    
&apos;do the same for arstrtarget
    strChar = Mid(strTarget, 1, 1)
    &apos; parse the string and put into arrays in order
    &apos; later make it so that it will end at the end of line or end of string!!
    Dim nArrayTarg As Long
    
    nArrayTarg = 0
    nIndex = 1
    While strChar &lt;&gt; &quot;@&quot;
        &apos; sample a,b,.n,aa,ii,fontname@
        &apos; go through each character
        &apos; if &apos;,&apos; is not found then we must add to the same array number
        &apos; if &apos;,&apos; is found we must increment the array number
        &apos; the string number is always incremented
        
        If strChar = &quot;,&quot; Then
            &apos;skip to the next letter on line
            strChar = Mid(strTarget, nIndex, 1)
            &apos;put in the next array as a new character for formating
            nArrayTarg = nArrayTarg + 1
        Else
            arstrTarget(nArrayTarg) = arstrTarget(nArrayTarg) + strChar
        End If
        &apos; prime the pump
        nIndex = nIndex + 1
        strChar = Mid(strTarget, nIndex, 1)
    Wend
    

    
    &apos; now do the search and replace stuff
    &apos; but first check to see if the array lengths are the same
    If (nArray &lt;&gt; nArrayTarg) Then
        strError = &quot;unequal csv elements please check two lines&quot; + strSource
        GoTo HandleError
    End If
    
    &apos; start at two because the first array is the number (of the field)
    For nCounter = 0 To nArray
        	call Myreplace( arstrSource(nCounter), arstrTarget(nCounter), bCase)
    &apos;return the loop for change font routine
    Next nCounter

    &apos;if the target font says nochange don&apos;t do it
    If arstrTarget(nArray) &lt;&gt; &quot;NO_CHANGE&quot; Then
         call ChangeFont (arstrTarget(nArray))
    End If
    
    &apos;check to see if an error was loaded. If so show it.
HandleError:
    If strError &lt;&gt; &quot;&quot; Then
       MsgBox (strError)
    End If
    
End Sub


sub Myreplace (sSource as string, sTarget as String, bCase as Boolean)
rem ----------------------------------------------------------------------
rem define variables
dim Doc   as object
dim Replace as object
rem ----------------------------------------------------------------------
rem get access to the document
Doc = ThisComponent
dim oSheets as object
Dim oSheet as object


if IsSPreadSheetDoc then
  
  call CalcReplaceByDesc(sSource, sTarget, bCase)
else
 Replace = Doc.createReplaceDescriptor

 Replace.SearchString = sSource
 Replace.ReplaceString = sTarget
 Replace.SearchCaseSensitive = bCase
 Doc.replaceAll(Replace)
endif



end sub

