sub CalcReplaceByDesc(sSource as string, sTarget as String, bCase as Boolean)
Dim oDescriptor 
Dim oFound 

dim oSheets as object
Dim oSheet as object
Dim oDoc as object

oDoc = ThisComponent
oSheet = oDoc.Sheets.getByName(&quot;Sheet1&quot;)

REM Create a descriptor from a searchable document. 
oDescriptor = oSheet.createReplaceDescriptor() 
With oDescriptor 
	.SearchString = sSource
	.ReplaceString = sTarget
	.SearchCaseSensitive = bCase
End With
 oSheet.replaceAll(oDescriptor)

 
 End Sub
 






